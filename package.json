{
  "name": "backend-bodas",
  "version": "1.0.0",
  "description": "Backend para la gestión de eventos y bodas con Express y MongoDB",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "lint": "eslint src --fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.1",
    "mongoose-unique-validator": "^4.0.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.3",
    "nodemailer": "^6.9.3",
    "npm": "^11.1.0",
    "puppeteer": "^24.4.0",
    "sharp": "^0.33.5",
    "venom-bot": "^5.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverage": true
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "puppeteer",
    "whatsapp",
    "wedding"
  ],
  "author": "Juan MAnuel GArcía del Toro",
  "license": "MIT"
}